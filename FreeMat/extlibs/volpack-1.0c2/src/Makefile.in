		echo "$(M4) -DSourceFile=$$i $(DEFAULT_HEADER) $(WARP_IN) > $$i" ;\
		$(M4) -DSourceFile=$$i $(DEFAULT_HEADER) $(WARP_IN) > $$i; \
		chmod -w $$i; \
	    done
