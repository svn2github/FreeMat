#!/bin/sh
mkdir mtex
cp ../MFiles/mprint*.m mtex/.
cd mtex
perl ../dcptrans.pl ../../libs/lib*/*.cpp
cp ../main.tex .
latex main
latex main
dvips -o main.ps main.dvi
pdflatex main
latex2html --split 5 -dir ../html -mkdir -noinfo -local_icons main.tex
cp *.png ../html/.
cd ..
perl bldindex.pl
cat toc_template.hhc toc_body.hhc > toc.hhc
cat Index_template.hhk index_body.hhk > index.hhk
